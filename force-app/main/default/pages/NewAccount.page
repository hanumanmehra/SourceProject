<apex:page controller="MyAccountController" sidebar="false" title="United oil & Gas Crop" standardStylesheets="True">

<apex:pagemessages /> 
    <apex:sectionHeader title="{!account.Name}"/>
    <apex:form >
        <chatter:feedWithFollowers showHeader="true" entityId="{!account.id}"/>
          
          <apex:pageBlock mode="maindetail" title="Account detail" tabStyle="Account" id="myId">
