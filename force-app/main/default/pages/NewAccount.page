                    <!-- <apex:commandButton action="{!edit}" id="editButton" value="edit"/>    -->
                    <apex:commandButton action="{!edit}" value="Edit" reRender="myId" />  
             </apex:pageBlockButtons>
    <apex:pageblockSection >
                    <apex:outputField value="{!account.name}"/>
