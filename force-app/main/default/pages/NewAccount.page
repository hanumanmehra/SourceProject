</apex:pageblockSection>
</apex:pageBlock>
</apex:form>


          <apex:pageBlock title="Contacts">
          <apex:form >
          <apex:pageblocktable value="{!listcont}" var="coli" Title="Contacts">
          <apex:column headerValue="Action">
           <apex:outputLink title="" value="/{!coli.id}/e?retURL=/apex/{!coli.Id}" style="font-weight:bold">Edit</apex:outputLink>
           </apex:column>

           
          <apex:column headerValue="Name">
          <apex:outputLink value="/{!coli.Id}" id="theLink">{!coli.Name}</apex:outputLink></apex:column>
     
          <apex:column value="{!coli.Email}"/>
          <apex:column value="{!coli.phone}"/>
          </apex:pageblocktable>
            </apex:form>
          </apex:pageBlock>
          
          <apex:pageBlock title="Cases">
          <apex:pageblocktable value="{!listcase}" var="caseli" Title="Cases">
          <apex:column value="{!caseli.CaseNumber}"/>
          <apex:column value="{!caseli.Subject}"/>
          <apex:column value="{!caseli.Priority}"/>
          <apex:column value="{!caseli.Status}"/>
          </apex:pageblocktable>
          </apex:pageBlock>

 
</apex:page>